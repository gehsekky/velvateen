{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-bitwise": 0,
    "camelcase": 1,
    "curly": 1,
    "no-eq-null": 0,
    "guard-for-in": 1,
    "no-empty": 1
  },
  "globals": {
    "$": false
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}